									<legend>문의 내용 작성 서식</legend>
									<div class="form-item">
										<div class="form-head"><div class="form-name">예금주 <span class="required">*</span></div></div>
										<div class="form-body">
											<div class="form-area">
												<span class="form theme-a style-b type-b normal-03 large flex">
													<input class="form-elem" type="text" placeholder="">
													<span class="form-util"><span class="form-clear"></span></span>
												</span>
											</div>
										</div>
									</div>
									<div class="form-item">
										<div class="form-head"><div class="form-name">은행명 <span class="required">*</span></div></div>
										<div class="form-body">
											<div class="form-area">
												<span class="form theme-a style-b type-b normal-03 large flex">
													<select class="form-elem">
														<option>은행 선택</option>
													</select>
												</span>
											</div>
										</div>
									</div>
									<div class="form-item">
										<div class="form-head"><div class="form-name">계좌번호 <span class="required">*</span></div></div>
										<div class="form-body">
											<div class="form-area">
												<span class="form theme-a style-b type-b normal-03 large flex">
													<input class="form-elem" type="text" pattern="[0-9]*" inputmode="numeric" placeholder="숫자만 입력 (- 제외)">
													<span class="form-util"><span class="form-clear"></span></span>
												</span>
											</div>
										</div>
									</div>
								</fieldset>
							</div>
