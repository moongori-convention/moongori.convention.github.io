<script>
var sorting = document.querySelectorAll('.sort-display .sort-item');
var sortingToggle = function() {
	var _this = this;
	
	for(var i = 0; i<sorting.length; i++) {
		sorting[i].classList.remove('current');
	}

	_this.classList.add('current');
}

for(var i = 0; i<sorting.length; i++) {
	sorting[i].addEventListener('click', sortingToggle);
}

</script>
