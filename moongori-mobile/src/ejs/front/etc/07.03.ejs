		var donloadCoupon = function(elem) {
			elem.classList.add('completed');
			elem.disabled = true;
			elem.innerHTML = '<span class="btn-text">다운완료</span>';
		}

		document.addEventListener('DOMContentLoaded', function() {
		});
	</script>
